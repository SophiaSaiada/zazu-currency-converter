{
  "name": "zazu-currency-converter",
  "version": "1.0.0",
  "description": "Zazu plugin for converting amounts between currencies.",
  "blocks": {
    "input": [
      {
        "id": "currency-convertor",
        "type": "RootScript",
        "script": "currency-convertor.js",
        "connections": ["Copy"]
      }
    ],
    "output": [
      {
        "id": "Copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}
